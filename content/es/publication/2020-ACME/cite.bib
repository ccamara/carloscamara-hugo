  ids = {leonPrecariedadComunesUrbanos2021},
  title = {{La precariedad de los comunes urbanos frente al derecho p\'ublico europeo}},
  author = {Le{\'o}n Casero, Jorge and {C{\'a}mara-Menoyo}, Carlos},
